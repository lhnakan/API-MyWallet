{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-unused-vars":"warn",
        "no-multiple-empty-lines": ["warn", { "max": 1 }],
        "space-before-function-paren": ["warn", "always"],
        "indent": ["warn", 4]
    }
}
